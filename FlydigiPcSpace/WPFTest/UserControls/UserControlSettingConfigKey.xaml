<UserControl x:Class="WPFTest.UserControls.UserControlSettingConfigKey"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WPFTest.UserControls"
             Height="650">
    <Grid>
        <local:UserControlSettingConfigKeyDisplay x:Name="mLayoutKeyDisplay" HorizontalAlignment="Left" Margin="320,0,0,0"/>
        <local:UserControlSettingConfigKeySetting x:Name="mLayoutKeySetting" HorizontalAlignment="Left" Visibility="Hidden"/>
        <Grid Name="mLayoutMappingKey" Width="320" Visibility="Visible" HorizontalAlignment="Left"
          Background="#181818">
            <StackPanel Margin="20,0,20,0" Orientation="Vertical">
                <local:UserControlTitle x:Name="mTitle1" Width="280" Height="24" Margin="0,20,0,0"/>
                <WrapPanel Name="mWrapPanel_1" Margin="0,10,0,10"/>
                <local:UserControlTitle x:Name="mTitle2" Width="280" Height="24"/>
                <WrapPanel Name="mWrapPanel_2" Margin="0,10,0,10"/>
                <local:UserControlTitle x:Name="mTitle3" Width="280" Height="24"/>
                <WrapPanel Name="mWrapPanel_3" Margin="0,10,0,0"/>
            </StackPanel>
        </Grid>
    </Grid>
</UserControl>